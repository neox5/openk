  "definitions": {
    "apiv1Status": {
      "type": "string",
      "enum": [
        "STATUS_UNSPECIFIED",
        "STATUS_OK",
        "STATUS_DEGRADED",
        "STATUS_UNHEALTHY"
      ],
      "default": "STATUS_UNSPECIFIED",
      "description": "- STATUS_UNSPECIFIED: STATUS_UNSPECIFIED represents an unknown state\n - STATUS_OK: STATUS_OK indicates the server is healthy\n - STATUS_DEGRADED: STATUS_DEGRADED indicates partial functionality\n - STATUS_UNHEALTHY: STATUS_UNHEALTHY indicates the server is not functioning",
      "title": "Status represents possible health states"
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "v1CheckResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/apiv1Status",
          "title": "Status represents the current server health status"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time",
          "title": "Timestamp indicates when the health check was performed"
        },
        "uptime": {
          "type": "string",
          "title": "Uptime indicates how long the server has been running"
        },
        "version": {
          "type": "string",
          "title": "Version contains build information (optional)"
        }
      },
      "title": "CheckResponse contains server health information"
    }
  }
